package com.jy.modules.drools.entity;
import java.util.List;

rule "Rule_A001"
	when
	   $list:List();
       $Current:Applicant();
       $DataBase:Applicant(this.custName==$Current.custName,this.birthDate==$Current.birthDate) from $list;
	then
	  System.out.println("two same objects in list:DataBase custName :" + $DataBase.getCustName());
end